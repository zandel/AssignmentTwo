<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>makePayment</servlet-name>
  	<display-name>makePayment</display-name>
  	<description></description>
  	<servlet-class>dbAccess.makePayment</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>dropme</servlet-name>
  	<display-name>dropme</display-name>
  	<description></description>
  	<servlet-class>dbAccess.dropme</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>makePayment</servlet-name>
  	<url-pattern>/makePayment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>dropme</servlet-name>
  	<url-pattern>/dropme</url-pattern>
  </servlet-mapping>
</web-app>
